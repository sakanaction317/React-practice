{
    "compilerOptions": {
      "target": "es6",
      "module": "ESNext",
      "strict": true,
      "esModuleInterop": true,
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true,
      "outDir": "./dist/scripts",
      "moduleResolution": "node"
    },
    "include": ["src/scripts/**/*.ts"]
  }
  